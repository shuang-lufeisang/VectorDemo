        android:width="24dp"
        android:height="24dp"
        android:viewportWidth="70.0"
        android:viewportHeight="70.0">


    <!-- 竖长椭圆 以50，50为起点，逆时针画椭圆图形，x轴半径10，y轴半径5 转动x轴~~~-->
    <path
        android:strokeWidth="0.3"
        android:strokeColor="#E6B90E"
        android:fillColor="#FF000000"
        android:pathData="M50,29 a10,5 90,1 0 1,0z"/>


    <!-- 横长椭圆 以50，50为起点，逆时针画椭圆图形，x轴半径10，y轴半径5 -->
    <path
        android:fillColor="#FF000000"
        android:strokeWidth="0.3"
        android:strokeColor="#E6B90E"
        android:pathData="M50,50 a10,5 0,1 0 1,0z"/>


    <path
        android:fillColor="#E6B90E"
        android:pathData="M20,20 l10,30 M30,20 l-10,30"/>
        <!--  -->


    <!--  正三角  -->
    <path
        android:strokeWidth="0.1"
        android:strokeColor="#E6B90E"
        android:fillColor="#FF000000"
        android:pathData="M7,34l5,-5 5,5z"/>


    <!--  画笔起点（20,0）；连线（20,40）；连线（40,40）-->
    <path
        android:name="draw"
        android:fillColor="#FF000000"
        android:pathData="M20,0 L20,40 40,40" />

    <group
        android:name="rotationGroup"
        android:pivotX="10.0"
        android:pivotY="10.0"
        android:rotation="15.0">

        <path
            android:name="vect"
            android:strokeWidth="0.1"
            android:strokeColor="#E6B90E"
            android:fillAlpha="0.3"
            android:fillColor="#FF000000"
            android:pathData="M15.67,4H14V2h-4v2H8.33C7.6,4 7,4.6 7,5.33V9h4.93L13,7v2h4V5.33C17,4.6 16.4,4 15.67,4z" />
        <path
            android:name="draw"
            android:strokeWidth="0.1"
            android:strokeColor="#E6B90E"
            android:fillColor="#FF000000"
            android:pathData="M13,12.5h2L11,20v-5.5H9L11.93,9H7v11.67C7,21.4 7.6,22 8.33,22h7.33c0.74,0 1.34,-0.6 1.34,-1.33V9h-4v3.5z" />
    </group>


</vector>

